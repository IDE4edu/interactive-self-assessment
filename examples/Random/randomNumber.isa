			<end>;</end>
		</textbox>
		
		<assertion>
			<method>matchParens</method>
			<target>randomNumber</target>
		</assertion>
		
		<assertion>
			<method>semiEveryLine</method>
			<target>randomNumber</target>
		</assertion>
		
		<assertion>
			<method>numLines</method>
			<target>randomNumber</target>
			<params>&lt;=2</params>
		</assertion>
		
		<test>
			<junit>randomNumberTest</junit>
		</test>
	
	</source>

</isa>